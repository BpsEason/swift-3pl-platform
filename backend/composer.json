{
    "name": "swift/3pl-platform",
    "description": "Swift 3PL Multi-Tenant Platform Core Demo.",
    "type": "project",
    "require": {
        "php": "^8.3",
        "laravel/framework": "^12.0",
        "stancl/tenancy": "^4.0", 
        "predis/predis": "^2.1",
        "maatwebsite/excel": "^3.1",
        "laravel/sanctum": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
